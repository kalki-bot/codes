package com.java;
class sample{
	public static void main(String[]args) {
		int matrix[][]= {{10,8,9},
				{17,50,8},
				{6,2,4},
				{5,1,3}};
		int avg=0;
		for(int[]row:matrix) {
			for(int num : row) {
				if(num%2==avg) {
					avg=num;
				}
				}	
					
			}
			System.out.println("average element:"+avg);
		}

		
}

